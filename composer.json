{
  "name": "lexide/reposition-sql",
  "description": "SQL module for Lexide Reposition",
  "license": "MIT",
  "homepage": "https://github.com/lexide/reposition-sql",
  "require": {
    "php": ">=5.5.0",
    "lexide/reposition": "^2.0.0",
    "lexide/clay": "^2.0.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Lexide\\Reposition\\Sql\\": "src",
      "Lexide\\Reposition\\Sql\\Test\\": "test"
    }
  },
  "suggest": {
    "lexide/puzzle-di": "Auto configured dependency injection for modules"
  },
  "extra": {
    "lexide/puzzle-di": {
      "lexide/syringe": {
        "path": "config/syringe.yml"
      }
    }
  }
}
